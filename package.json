{
  "name": "create-quickstart",
  "version": "1.7.0",
  "description": "create quickstart project",
  "main": "index.js",
  "bin": {
    "create-quickstart": "bin/create-quickstart"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liuyuchenzh/create-quickstart.git"
  },
  "bugs": {
    "url": "https://github.com/liuyuchenzh/create-quickstart/issues"
  },
  "scripts": {
    "create-quickstart": "node ./bin/create-quickstart",
    "format": "pretty-quick",
    "commit": "git-cz"
  },
  "engines": {
    "node": ">=8.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "create-quickstart"
  ],
  "author": "Yuchen Liu",
  "license": "MIT",
  "dependencies": {
    "commander": "^5.0.0",
    "fs-extra": "^9.0.0",
    "husky": "^4.0.0",
    "inquirer": "^7.0.0",
    "lint-staged": "^10.0.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
