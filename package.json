{
  "name": "create-quickstart",
  "version": "1.5.0",
  "description": "create quickstart project",
  "main": "index.js",
  "bin": {
    "create-quickstart": "bin/create-quickstart"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liuyuchenzh/y-cli.git"
  },
  "bugs": {
    "url": "https://github.com/liuyuchenzh/y-cli/issues"
  },
  "scripts": {
    "create-quickstart": "node ./bin/create-quickstart",
    "format": "pretty-quick",
    "commit": "git-cz"
  },
  "engines": {
    "node": ">=8.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "y-cli"
  ],
  "author": "Yuchen Liu",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.19.0",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "inquirer": "^6.2.2",
    "lint-staged": "^8.1.5",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
